{
  "name": "react_blog_project",
  "version": "1.0.0",
  "description": "This project is a Creative Blog Website that allows users to view other posts by others and generate their own post and AI imnages with our AI generator.",
  "main": "index.js",
  "scripts": {
    "start:frontend": "react-scripts start",
