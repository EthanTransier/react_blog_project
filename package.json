    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "keywords": [
    "Blog",
    "Social Media",
    "React"
  ],
  "author": "Ethan Transier (https://github.com/EthanTransier)",
  "contributors": [
    {
      "name": "Georgiana Barefield (https://github.com/gbaref949)"
    }
  ],
  "license": "MIT",
  "dependencies": {
